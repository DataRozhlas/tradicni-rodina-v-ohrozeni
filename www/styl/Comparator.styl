.comparator
  .zero-line
    stroke black
    shape-rendering crispEdges

  .paths
    path
      fill none
      stroke #a
      stroke-width 1
      // stroke-opacity 0.5
      &.civil
        stroke prideOrange
        stroke-width 2
      &.marriage
        stroke prideRed
        stroke-width 2
    .country:hover path
      stroke-width 3
    .country.slovakia
      path
        stroke-width 2
        stroke black
